
<%= form_for @user do |f| %>

<%= f.label :grade, '学年' %>
<%= f.text_field :grade %>
<br />

<%= f.label :grade, '学籍番号' %>
<%= f.text_field :snum %>
<br />

<%= f.label :grade, '氏名' %>
